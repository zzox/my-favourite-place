{
    "defaultSeverity": "INFO",
    "extendsConfigPath": "",
    "numberOfCheckerThreads": 5,
    "baseDefines": [],
    "defineCombinations": [],
    "checks": [
        {
            "type": "Anonymous",
            "props": {}
        },
        {
            "type": "AvoidStarImport",
            "props": {}
        },
        {
            "type": "BlockBreakingConditional",
            "props": {}
        },
        {
            "type": "CodeSimilarity",
            "props": {
                "severityIdentical": "WARNING",
                "thresholdIdentical": 60,
                "thresholdSimilar": 120
            }
        },
        {
            "type": "ConstantName",
            "props": {
                "format": "^[A-Z][A-Z0-9]*(_[A-Z0-9_]+)*$",
                "tokens": [
                    "INLINE"
                ],
                "ignoreExtern": false
            }
        },
        {
            "type": "ConstantName",
            "props": {
                "format": "^[A-Z][A-Z0-9]*(_[A-Z0-9_]+)*$",
                "tokens": [
                    "NOTINLINE"
                ],
                "ignoreExtern": false
            }
        },
        {
            "type": "CyclomaticComplexity",
            "props": {
                "thresholds": [
                    {
                        "severity": "WARNING",
                        "complexity": 32
                    },
                    {
                        "severity": "ERROR",
                        "complexity": 40
                    }
                ]
            }
        },
        {
            "type": "Dynamic",
            "props": {}
        },
        {
            "type": "EmptyLines",
            "props": {
                "max": 1,
                "allowEmptyLineAfterSingleLineComment": true,
                "allowEmptyLineAfterMultiLineComment": true,
                "requireEmptyLineAfterPackage": false,
                "requireEmptyLineAfterClass": false,
                "requireEmptyLineAfterInterface": false,
                "requireEmptyLineAfterAbstract": false
            }
        },
        {
            "type": "ExtendedEmptyLines",
            "props": {
                "max": 1,
                "skipSingleLineTypes": true,
                "defaultPolicy": "upto",
                "ignore": [],
                "none": [
                    "beforePackage",
                    "betweenImports",
                    "beforeUsing",
                    "typeDefinition",
                    "afterLeftCurly",
                    "beforeRightCurly"
                ],
                "exact": [],
                "upto": [],
                "atleast": []
            }
        },
        {
            "type": "Final",
            "props": {}
        },
        {
            "type": "Indentation",
            "props": {
                "character": "    ",
                "ignoreConditionals": false,
                "conditionalPolicy": "ignore",
                "ignoreComments": false,
                "wrapPolicy": "larger"
            }
        },
        {
            "type": "IndentationCharacter",
            "props": {
                "character": "space",
                "ignorePattern": "^$"
            }
        },
        {
            "type": "LeftCurly",
            "props": {
                "tokens": [
                    "CLASS_DEF",
                    "ENUM_DEF",
                    "ABSTRACT_DEF",
                    "INTERFACE_DEF",
                    "FUNCTION",
                    "FOR",
                    "IF",
                    "WHILE",
                    "SWITCH",
                    "TRY",
                    "CATCH"
                ],
                "option": "eol",
                "ignoreEmptySingleline": true,
                "ignoreSingleline": false
            }
        },
        {
            "type": "LineLength",
            "props": {
                "max": 120,
                "ignorePattern": "^$"
            }
        },
        {
            "type": "MethodLength",
            "props": {
                "max": 100,
                "ignoreEmptyLines": true
            }
        },
        {
            "type": "NestedControlFlow",
            "props": {
                "max": 4
            }
        },
        {
            "type": "NestedIfDepth",
            "props": {
                "max": 2
            }
        },
        {
            "type": "OperatorWhitespace",
            "props": {
                "assignOpPolicy": "around",
                "unaryOpPolicy": "none",
                "ternaryOpPolicy": "ignore",
                "arithmeticOpPolicy": "around",
                "compareOpPolicy": "around",
                "bitwiseOpPolicy": "ignore",
                "boolOpPolicy": "around",
                "intervalOpPolicy": "ignore",
                "arrowPolicy": "ignore",
                "arrowFunctionPolicy": "ignore",
                "oldFunctionTypePolicy": "ignore",
                "newFunctionTypePolicy": "ignore"
            }
        },
        {
            "type": "ParameterNumber",
            "props": {
                "max": 6,
                "ignoreOverriddenMethods": false
            }
        },
        {
            "type": "RedundantAccessMeta",
            "props": {
                "prohibitMeta": false
            }
        },
        {
            "type": "RedundantAllowMeta",
            "props": {
                "prohibitMeta": false
            }
        },
        {
            "type": "RedundantModifier",
            "props": {
                "enforcePublicPrivate": false,
                "enforcePublic": false,
                "enforcePrivate": false
            }
        },
        {
            "type": "Return",
            "props": {
                "allowEmptyReturn": true,
                "enforceReturnType": false,
                "enforceReturnTypeForAnonymous": false
            }
        },
        {
            "type": "RightCurly",
            "props": {
                "tokens": [
                    "CLASS_DEF",
                    "ENUM_DEF",
                    "ABSTRACT_DEF",
                    "TYPEDEF_DEF",
                    "INTERFACE_DEF",
                    "OBJECT_DECL",
                    "FUNCTION",
                    "FOR",
                    "WHILE",
                    "SWITCH",
                    "TRY",
                    "CATCH"
                ],
                "option": "aloneorsingle"
            }
        },
        {
            "type": "RightCurly",
            "props": {
                "tokens": [
                    "IF"
                ],
                "option": "same"
            }
        },
        {
            "type": "SeparatorWhitespace",
            "props": {
                "dotPolicy": "none",
                "commaPolicy": "after",
                "allowTrailingComma": false,
                "semicolonPolicy": "after"
            }
        },
        {
            "type": "SeparatorWrap",
            "props": {
                "tokens": [
                    ","
                ],
                "option": "eol"
            }
        },
        {
            "type": "Spacing",
            "props": {
                "spaceAroundBinop": false,
                "noSpaceAroundUnop": false,
                "spaceIfCondition": "should",
                "spaceForLoop": "any",
                "spaceWhileLoop": "any",
                "spaceSwitchCase": "any",
                "spaceCatch": "any",
                "ignoreRangeOperator": false
            }
        },
        {
            "type": "StringLiteral",
            "props": {
                "policy": "onlySingle",
                "allowException": true
            }
        },
        {
            "type": "TODOComment",
            "props": {
                "format": "^\\s*(ATTN|PROD|TODO|FIXME|HACK|XXX|BUG|MD)"
            }
        },
        {
            "type": "Trace",
            "props": {}
        },
        {
            "type": "TrailingWhitespace",
            "props": {}
        },
        {
            "type": "Type",
            "props": {
                "ignoreEnumAbstractValues": true
            }
        },
        {
            "type": "UnnecessaryConstructor",
            "props": {}
        },
        {
            "type": "UnusedLocalVar",
            "props": {}
        }
    ],
    "exclude": {},
    "version": 1
}